#include<iostream>
using namespace std;

int partition(int arr[],int start,int end)
{
    int pos=start;
    for (int i=start;i<=end;i++){
        if(arr[i]<=arr[end])
        {
            swap(arr[i],arr[pos]);
            pos++;
        }
    }
    return pos-1;
}
void quicksort(int arr[],int start,int end)
{
    if(start>=end)
    return;
    int pivot=partition(arr,start,end);
    //left part
    quicksort(arr,start,pivot-1);
    //right part
    quicksort(arr,pivot,end);
}
int main(){
    int arr[]={45,16,0,4,7,12,85,19,36,0,4,23};
    quicksort(arr,0,12);
    for(int i=0;i<12;i++)
    {
        cout<<arr[i]<<" ";
    }
}
